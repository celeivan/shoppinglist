{
    "name": "ivan/my-shopping-app",
    "type": "project",
    "autoload": {
        "psr-4": {
            "MyShoppingApp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyShoppingApp\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "vlucas/phpdotenv": "^5.5"
    }
}
